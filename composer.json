{
	"name": "sibyx/phpgpx",
	"type": "library",
	"version": "2.0.0",
	"description": "A simple PHP library for GPX import/export",
	"minimum-stability": "stable",
	"license": "MIT",
	"authors": [
		{
			"name": "Jakub Dubec",
			"email": "jakub.dubec@gmail.com",
			"homepage": "https://jakubdubec.me"
		}
	],
	"require": {
		"php": ">=7.1",
		"lib-libxml": "*",
		"ext-simplexml": "*",
		"ext-dom": "*",
		"ext-json": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5.0"
	},
	"autoload": {
		"psr-4": { "phpGPX\\": "src/phpGPX/" }
	},
	"autoload-dev": {
		"psr-4": { "phpGPX\\Tests\\": "tests/" }
	}
}
